{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft.AspNetCore": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Serilog LOGs\\logfile.log"
        }
      }
    ]
  },

  "ConnectionStrings": {
    "ModsenAppDataBase": "Server=localhost\\SQLEXPRESS01;Database=ModsenAppDB;Trusted_Connection=True;"
  },

  "AllowedHosts": "*",
  "IdentityUrl": "https://localhost:10001"
}
